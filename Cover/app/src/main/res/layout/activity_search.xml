<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lan.cover.Search11"
    android:id="@+id/m1">


    <FrameLayout android:id="@+id/container" android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="0dp"
        android:background="@drawable/cfbb0da308a8c6f79e9b1909d60da846">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:id="@+id/editText"
            android:layout_gravity="left|top"
            android:background="#5fff4c"
            android:text="               搜索"
            android:textColor="#fffdf3"
            android:textSize="40sp" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:id="@+id/editText2"
            android:layout_gravity="left|bottom"
            android:background="#5fff4c" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/搜索">

            <FrameLayout
                android:layout_width="94dp"
                android:layout_height="50dp"
                android:layout_gravity="left|bottom">

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:id="@+id/btn1"
                    android:layout_gravity="center"
                    android:background="@drawable/zhuye1" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:layout_gravity="right|bottom">

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:id="@+id/btn4"
                    android:layout_gravity="center"
                    android:background="@drawable/wode1" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="142dp"
                android:layout_height="53dp"
                android:layout_gravity="center_horizontal|bottom">

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:id="@+id/btn2"
                    android:layout_gravity="left|center_vertical"
                    android:background="@drawable/faxian1" />

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:id="@+id/btn3"
                    android:layout_gravity="right|center_vertical"
                    android:background="@drawable/sousuo2" />
            </FrameLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="368dp"
                android:layout_gravity="center"
                android:weightSum="1">

                <EditText
                    android:layout_width="234dp"
                    android:layout_height="wrap_content"
                    android:inputType="textMultiLine"
                    android:ems="10"
                    android:id="@+id/editText4"
                    android:layout_gravity="center_horizontal"
                    android:drawableLeft="@drawable/sousuo1"/>

            </LinearLayout>

        </FrameLayout>

    </FrameLayout>
</RelativeLayout>
